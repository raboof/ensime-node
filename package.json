{
  "name": "Ensime",
  "main": "./lib/ensime",
  "version": "0.31.1",
  "description": "Ensime integration for atom",
  "repository": "https://github.com/ensime/ensime-atom",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "atom-message-panel": "~1.2.4",
    "atom-package-dependencies": "latest",
    "atom-space-pen-views": "^2.1.0",
    "bluebird": "^3.0.6",
    "diff": "^2.2.1",
    "emissary": "1.x",
    "jquery": "^2",
    "lodash": "~3.9.3",
    "recursive-readdir": "^1.3.0",
    "scandal": "^2.2.0",
    "sub-atom": "~1.0.0",
    "vue": "1.0.0-csp"
  },
  "package-dependencies": {
    "hyperclick": "whatever",
    "language-scala": "whatever"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "hyperclick.provider": {
      "versions": {
        "0.0.0": "provideHyperclick"
      }
    }
  },
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "value": "unix",
